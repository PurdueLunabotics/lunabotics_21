<launch>

<rosparam file="$(find lunabotics_sim_v2_description)/launch/controller.yaml" command="load"/>
<node args="back_right_position_controller front_left_position_controller front_right_position_controller back_left_position_controller joint_state_controller " name="controller_spawner" ns="lunabotics_sim_v2" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/lunabotics_sim_v2/joint_states"/>
</node>

</launch>